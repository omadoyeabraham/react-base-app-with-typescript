{
  "name": "the-productive-life-web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "autoprefixer": "^9.8.6",
    "chokidar-cli": "^2.1.0",
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "^2.22.0",
    "react-query-devtools": "^2.4.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.7.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "build:css": "postcss src/assets/css/main.imports.css -o src/assets/css/main.css",
    "watch:css": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/assets/css/main.css -c 'yarn run build:css' ",
    "start": "npm-run-all build:css --parallel watch:css start:react",
    "start:react": "react-scripts start",
    "prebuild": "run-s build:css",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-actions": "^6.0.12",
    "@storybook/addon-essentials": "^6.0.12",
    "@storybook/addon-links": "^6.0.12",
    "@storybook/node-logger": "^6.0.12",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.12",
    "babel-loader": "^8.1.0",
    "react-is": "^16.13.1"
  }
}
